{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug Express Server",
      "program": "${workspaceFolder}/server/index.mjs",
      "runtimeArgs": [
        "--experimental-modules"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug Gulp Task",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": [
        "optimize-images"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
